#include<stdio.h>
int main()
{
int i,n;
int term1=0,term2=1;
int nextTerm= term1=term2;
printf("num of terms is:");
scanf("%d",&n);
printf("the fibonacci series is : %d, %d,",term1,term2);
for(i=3; i <=n;++i)
{
printf("%d," , nextTerm);
{
term1 =term2;
term2=nextTerm;
nextTerm = term1 + term2;
}
return 0;
}
}   
